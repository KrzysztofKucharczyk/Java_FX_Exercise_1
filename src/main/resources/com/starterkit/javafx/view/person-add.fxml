<?xml version="1.0" encoding="UTF-8"?>

<?scenebuilder-preview-i18n-resource ../bundle/base.properties?>
<?scenebuilder-stylesheet ../css/standard.css?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="290" prefWidth="500" xmlns="http://javafx.com/javafx/8.0.51" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.starterkit.javafx.controller.AddBookController">
  <Label layoutX="14.0" layoutY="14.0" maxWidth="1.7976931348623157E308" minWidth="-Infinity" prefWidth="474.0" styleClass="formTitle" text="%label.formTitle" />
  <GridPane layoutX="14.0" layoutY="60.0" hgap="10.0" vgap="10.0">
    <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="300.0" />
    </columnConstraints>
    <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <children>
      <Label text="%label.title" />
      <TextField fx:id="newTitleField" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" />
      <Label text="%label.authors" GridPane.rowIndex="1" />
      <TextField fx:id="newAuthorsField" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
      <Label text="%label.status" GridPane.rowIndex="2"/>
      <TextField fx:id="newStatusField" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
      <Label text="%label.genre" GridPane.rowIndex="3"/>
      <TextField fx:id="newGenreField" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
      <Label text="%label.year" GridPane.rowIndex="4"/>
      <TextField fx:id="newYearField" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.rowIndex="4"/>
    </children>
  </GridPane>
  <Button fx:id="closeWindowButton" defaultButton="false" layoutX="14.0" layoutY="250.0" mnemonicParsing="false" onAction="#closeWindowButtonAction" text="%button.cancel" />
  <Button fx:id="saveBookButton" defaultButton="true" layoutX="114.0" layoutY="250.0" mnemonicParsing="false" onAction="#saveButtonAction" text="%button.save" />
</AnchorPane>
